
<% lista.forEach((publi) => { %>
<div class="card">
    <div class="card-header">
        <a href="/blog/detail/<%= publi._id %>"><%= publi.titulo %></a>
    </div>
    <div class="card-body">
      <blockquote class="blockquote mb-0">
        <p><%= publi.descripcion %></p>
        <footer class="blockquote-footer"><%= publi.categoria  %> - <cite title="Source Title"><%= publi.fecha %></cite></footer>
      </blockquote>
    </div>
</div> 
<br>      
<% }); %>
